  procedures :
   -
     name : Constructeur
     procedure_id : 2203475646338668602
     type_code : 27
     code : |1-
      // Résumé : Permet d'initialiser la classe avec l'ensemble des informations d'une colonne de table.
      // Syntaxe : Constructeur (<pclBaseDeDonnées> est objet dynamique, <sTableNomUnique> est chaîne, <sNomOriginal> est chaîne, <nOrdrePhysique> est entier, <nTypeWD> est entier, <sType> est chaîne, <nTaille> est entier [, <bEstNullable> est booléen [, <sFormuleCalcule> est chaîne [, <bFormulePersiste> est booléen [, <nIndices> est entier [, <sLibellé> est chaîne [, <xValeurDéfaut> [, <sGUID> est chaîne [, <bEtatEtRequête> est booléen [, <nVignettes> est entier [, <tabVignette> est tableau [, <sTypeCombiEnum> est chaîne [, <sLangue> est chaîne [, <nTimeStamp> est entier [, <bTimeStamp> est booléen [, <nOrdreLogique> est entier [, <sDomaine> est chaîne]]]]]]]]]]]]]]]])
      // Paramètres :
      //	pclBaseDeDonnées (objet dynamique) : Base de données associée à la colonne.
      //	sTableNomUnique (chaîne UNICODE) : Nom unique de la table.
      //	sNomOriginal (chaîne UNICODE) : Nom original de la colonne dans la base de données.
      //	nOrdrePhysique (entier) : Ordre physique de la colonne dans la table.
      //	nTypeWD (entier) : Type WinDev de la colonne (voir hRubxxx)
      //	sType (chaîne UNICODE) : Type original de la colonne.
      //	nTaille (entier) : Taille de la colonne.
      // 	bEstNullable (booléen optionnel) : Indique si la colonne autorise les valeurs nulles (par défaut à Faux).
      //	sFormuleCalcule (chaîne UNICODE optionnelle) : Formule de calcul associée à la colonne (par défaut à "").
      // 	bFormulePersiste (booléen optionnel) : Indiquant si la formule de calcul doit être persistante (par défaut à Faux).
      // 	nIndices (entier optionnel) : Nombre d'indices de la colonne (par défaut à 0).
      //	sLibellé (chaîne UNICODE optionnelle) : Libellé de la colonne (par défaut à "").
      // 	xValeurDéfaut (valeur par défaut=0) : Valeur par défaut de la colonne (par défaut à Null).
      //	sGUID (chaîne UNICODE optionnelle) : GUID associé à la colonne (par défaut à "").
      // 	bEtatEtRequête (booléen optionnel) : Indique si la colonne fait partie de l'état ou de la requête (par défaut à Faux).
      // 	nVignettes (entier optionnel) : Nombre de vignettes associées à la colonne (par défaut à 0).
      // 	tabVignette (tableau optionnel) : Informations sur les vignettes (par défaut à Null).
      //	sTypeCombiEnum (chaîne UNICODE optionnelle) : Type de combinaison ou d'énumération associé à la colonne (par défaut à "").
      //	sLangue (chaîne UNICODE optionnelle) : Langue associée à la colonne (par défaut à "").
      // 	nTimeStamp (entier optionnel) : Timestamp associé à la colonne (par défaut à htsAucun).
      // 	bTimeStamp (booléen optionnel) : Indique si le timestamp est en mode UTC (par défaut à Faux).
      // 	nOrdreLogique (entier optionnel) : Ordre logique de la colonne (par défaut à 0).
      //	sDomaine (chaîne UNICODE optionnelle) : Domaine de la colonne (par défaut à "").
      // Valeur de retour : 	Aucune
      //
      PROCEDURE Constructeur(pclBaseDeDonnées est un objet dynamique,sTableNomUnique est une chaine,sNomOriginal est une chaine,nOrdrePhysique est un entier,nTypeWD est un entier,sType est une chaine,...
      	nTaille est un entier,bEstNullable est un booleen = faux,sFormuleCalcule est une chaine = "",bFormulePersiste est un booleen=faux,nIndices est un entier = 0,sLibellé est une chaine = "",...
      	xValeurDéfaut = null,sGUID est une chaine = "",bEtatEtRequête est un booleen = faux, nVignettes est un entier = 0,tabVignette est un tableau de ccolonne.STHFVignette = null,...
      	sTypeCombiEnum est une chaine = "",	sLangue est une chaine = "",nTimeStamp est un entier = htsAucun,bTimeStampUTC est un booleen = faux,nOrdreLogique est un entier=0,sDomaine est une chaîne ="")
      
      SI cApplication._Application<>Null _ET_ cApplication._Application.m_pclTrace<>Null ALORS cApplication._Application.m_pclTrace.AjouteMéthode(sTableNomUnique,sNomOriginal,nOrdrePhysique,nTypeWD,sType,nTaille,bEstNullable,sFormuleCalcule,bFormulePersiste,sLibellé,xValeurDéfaut)
      
      m_sTableNomUnique				= sTableNomUnique
      m_sNomOriginal					= sNomOriginal
      m_sNom							= minuscule(m_sNomOriginal)
      m_sNomUnique					= m_sTableNomUnique+cgenerique._Sep+m_sNom
      m_sDomaine						= sDomaine
      m_nOrdrePhysique				= nOrdrePhysique
      m_nhfOrdreLogique				= nOrdreLogique
      m_nTypeWD						= nTypeWD
      m_sTypeOriginal					= sType
      m_nTaille						= nTaille
      m_bEstNullable					= bEstNullable
      m_sFormuleCalcule				= sFormuleCalcule
      m_bHFPersistanceRubriqueCalculée= bFormulePersiste
      m_nIndices						= nIndices
      m_sLibellé						= sLibellé
      m_sValeurDéfaut					= xValeurDéfaut
      m_sGUID							= sGUID
      m_bÉtatEtRequête				= bEtatEtRequête
      m_nHFNbVignettesDécrit			= nVignettes
      si tabVignette<>null alors m_tabHFVignettes	= tabVignette
      m_sHFTypeCombinaisonÉnumération	= sTypeCombiEnum
      m_sHFLangue						= sLangue	//3 valeurs, attention peut contenir "##CREATION##","##BINARY##","##FASTSORT##"
      m_nHFTimeStampType				= nTimeStamp
      m_bHFTimeStampUTC				= bTimeStampUTC
      m_sNomSQL						= (pclBaseDeDonnées.SQLTableAvantColonne() ? cApplication.mg_taInfo[m_sTableNomUnique].m_sPremierAlias+"." sinon "")+pclBaseDeDonnées.SQLAvantColonne()+Remplace(m_sNomOriginal,",",pclBaseDeDonnées.SQLAprèsColonne()+","+pclBaseDeDonnées.SQLAvantColonne())+pclBaseDeDonnées.SQLAprèsColonne()
      m_bEstAutomatique				= (m_nTypeWD DANS (hRubIdAuto,hRubIdAuto4) _ou_ m_sFormuleCalcule>"")
      
      cApplication.mg_taInfo[m_sTableNomUnique].p_sToutesLesColonnes					= m_sNom
      SI m_nTypeWD DANS (hRubMémoBinaire,hRubMémoBinaire4,hRubImage) ALORS
      	cApplication.mg_taInfo[m_sTableNomUnique].p_sToutesLesColonnesMémoBinaire	= m_sNom
      SINON SI m_nTypeWD DANS (hRubMémoTexte,hRubMémoUnicode) alors
      	cApplication.mg_taInfo[m_sTableNomUnique].p_sToutesLesColonnesMémoTexte		= m_sNom
      SINON
      	cApplication.mg_taInfo[m_sTableNomUnique].p_sToutesLesColonnesNonMémo		= m_sNom
      FIN
