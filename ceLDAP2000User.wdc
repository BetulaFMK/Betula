  properties :
   -
     name : p_bCompteActif
     identifier : 0x11f7e79910087f04
     type_code : 103
     p_codes :
      -
        code : |1-
         //
         PROCEDURE PUBLIQUE p_bCompteActif() : booléen
         SI userAccountControl>0 ALORS
         	RENVOYER (PAS userAccountControl DANS (cLDAP2000User._ADUserAccountDisabled,cLDAP2000User._ADUserAccountDisabledPasswordNotExpire) ET isDeleted<>"1")
         SINON SI dn>"" ALORS
         	RENVOYER (PAS Position(dn,",OU=Disabled Accounts,")>0)
         FIN
         RENVOYER Faux
        type : 1966080
      -
        code : |1+
         //
         PROCEDURE PUBLIQUE p_bCompteActif(Valeur)
         SI Valeur ALORS 
         	SI userAccountControl=cLDAP2000User._ADUserAccountDisabled ALORS userAccountControl=cLDAP2000User._ADUserAccountNormalAccount
         	SI userAccountControl=cLDAP2000User._ADUserAccountDisabledPasswordNotExpire ALORS userAccountControl=cLDAP2000User._ADUserAccountNormalAccount+cLDAP2000User._ADUserAccountDontExpirePassword
         SINON
         	SI userAccountControl=cLDAP2000User._ADUserAccountNormalAccount ALORS userAccountControl=cLDAP2000User._ADUserAccountDisabled
         	SI userAccountControl=cLDAP2000User._ADUserAccountNormalAccount+cLDAP2000User._ADUserAccountDontExpirePassword ALORS userAccountControl=cLDAP2000User._ADUserAccountDisabledPasswordNotExpire
         FIN
        type : 2031616
