      //Classe User pour LDAP sur serveur Windows 2000.
      
      CONSTANTE
      	SAM_DOMAIN_OBJECT 				= 0x0
      	SAM_GROUP_OBJECT 				= 0x10000000
      	SAM_NON_SECURITY_GROUP_OBJECT 	= 0x10000001
      	SAM_ALIAS_OBJECT 				= 0x20000000
      	SAM_NON_SECURITY_ALIAS_OBJECT 	= 0x20000001
      	SAM_USER_OBJECT 				= 0x30000000
      	SAM_NORMAL_USER_ACCOUNT 		= 0x30000000
      	SAM_MACHINE_ACCOUNT 			= 0x30000001
      	SAM_TRUST_ACCOUNT 				= 0x30000002
      	SAM_APP_BASIC_GROUP 			= 0x40000000
      	SAM_APP_QUERY_GROUP 			= 0x40000001
      	SAM_ACCOUNT_TYPE_MAX 			= 0x7fffffff
      FIN
      
      ceLDAP2000User est une Classe
      	hérite de ceLDAPUser
      	hérite de cLDAP2000Top
      	hérite de cLDAP2000Security
      	hérite de cLDAP2000User
      	hérite de cLDAP2000Employee
      	hérite de cLDAP2000Mail
      GLOBAL CONSTANT
      	mg_sLDAP2000UserAttributs			est une chaine <sérialise = faux>
      FIN
