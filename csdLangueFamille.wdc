      pclFamille = allouer un ceLangueFamille("construite,planifiée,idéolangue,artificielle,conlang","constructed,conlang");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("créole","creole");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("dené–ienisseïenne","dené–Yeniseian");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("dravidienne","dravidian");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("eskimo-aléoute,inuite-yupik-unangax,eskaléoute,inuite-aléoute","eskimo–aleut,inuit–yupik–unangan,eskaleut");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("indo-européenne","indo-european");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("japonique","japonic,japanese–ryukyuan,japanic");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("kartvélienne","kartvelian,south caucasian");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("coréenne,coréanique","koreanic");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("isolat","isolate");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("mongole,mongolique","mongolic");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("nigéro-congolaise,niger-congo","niger-congo");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("nilo-saharienne","nilo-saharan");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("nakho-daghestanienne,caucasiennee du Nord-Est,nakho-daghestanaises,nakho-daguestaniennes,nakho-daguestanaises","northeast caucasian,east caucasian,nakh-daghestanian");TableauAjoute(mg_tabFamilles,pclFamille)
