      pclFamille = allouer un ceLangueFamille("taï-kadaï,daïque","kra–dai,tai–kadai,daic");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("tupi,tupie","tupi,tupian");TableauAjoute(mg_tabFamilles,pclFamille)
      pclFamille = allouer un ceLangueFamille("turcique","turkic");TableauAjoute(mg_tabFamilles,pclFamille)
