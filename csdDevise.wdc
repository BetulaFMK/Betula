  procedures :
   -
     name : Constructeur
     procedure_id : 1347197207805854954
     type_code : 27
     code : |1-
      // Résumé : Initialise une instance de la classe
      // Syntaxe : Constructeur (<nIndiceDeConnexion> est entier [, <sFeuille> est chaîne])
      // Paramètres :
      //	nIndiceDeConnexion (entier) : Indique l'indice de connexion pour accéder au fichier Excel contenant les données des devises.
      //	sFeuille (chaîne UNICODE optionnelle) : Nom de la feuille Excel à partir de laquelle les données des devises seront extraites. La valeur par défaut est "Feuil1".
      // Valeur de retour : Aucune
      //
      PROCEDURE Constructeur(nIndiceDeConnexion est une entier,sFeuille est une chaine = "Feuil1")
      cSourceDeDonnées.Constructeur(sFeuille,nIndiceDeConnexion,mg_tabDevises,vrai)
      cApplication.mg_taInfo[m_sTableNomUnique].p_sColonnesCléPrimaire = "A"
