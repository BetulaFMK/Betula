  procedures :
   -
     name : Constructeur
     procedure_id : 1344192817980338577
     type_code : 27
     code : |1-
      // Résumé : Initialise une instance de la classe cAPI.
      // Syntaxe : Constructeur (<sURLDeBase> est chaîne, <sCléAPI> est chaîne [, <duTimeOut> est durée])
      // Paramètres :
      //	sURLDeBase (chaîne UNICODE) : URL de base de la classe qui hérite
      //	sCléAPI (chaîne UNICODE) : La clé API pour permettre l'accès au service
      //	duTimeOut (durée optionnelle) : Durée après laquelle le timeout se déclenche, par défaut : 5 secondes
      // Valeur de retour : Aucune
      // Exemple :
      //
      PROCEDURE Constructeur(sURLDeBase est une chaine, sCléAPI est une chaine,duTimeOut est une durée = 5s)
      cHTTPAPI.constructeur("https://api.apilayer.com/"+sURLDeBase+"/")	
      m_sCléAPI 	= sCléAPI
      m_duTimeOut = duTimeOut
     type : 589824
   -
     name : Destructeur
     procedure_id : 1344192817980404113
     type_code : 28
     code : |1-
      // Résumé : Libère les ressources associées à l'instance de la classe.
      // Syntaxe : Destructeur ()
